{
  "name": "zzs-data-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "node --env-file=.env dist/server.js",
    "dev": "node --env-file=.env --import=tsx --watch-path . src/server.ts",
    "build": "rm -rf dist & tsc --project tsconfig.json",
    "test": "pnpm test:node && pnpm test:jest",
    "test:node": "node --import=tsx --test ./**/*.node-test.*",
    "test:jest": "jest --passWithNoTests",
    "test:coverage": "c8 pnpm test:node",
    "lint:check": "biome check",
    "lint:fix": "biome format --write",
    "lint:pre-commit": "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true --colors=off"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "c8": "^10.1.3",
    "jest": "^29.7.0",
    "lefthook": "^1.9.3",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  }
}
